{
  "name": "hashtag-generator",
  "version": "1.0.0",
  "description": "A hashtag generating api",
  "main": "src/client/index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack-dev-server --config webpack.dev.js --open",
    "start": "node src/server/server.js",
    "dev": "nodemon src/server/server.js",
    "debug": "nodemon --inspect src/server/server.js",
    "all": "npm-run-all --parallel \"build-dev\" \"dev\"",
    "debug-all": "npm-run-all --parallel \"build-dev\" \"debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LalaIAm/hashtag-gen.git"
  },
  "keywords": [
    "webpack",
    "nlp",
    "javascript"
  ],
  "author": "Lauren Thorud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LalaIAm/hashtag-gen/issues"
  },
  "homepage": "https://github.com/LalaIAm/hashtag-gen#readme",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-config-strongloop": "^2.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "superagent": "^5.2.2",
    "supertest": "^4.0.2",
    "terser-webpack-plugin": "^3.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "aylien_textapi": "^0.7.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-junit"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": ".",
    "outputName": "junit.xml",
    "uniqueOutputName": "false",
    "usePathForSuiteName": "true"
  }
}
